{
  "name": "organized",
  "main": "./lib/organized",
  "version": "0.4.2",
  "description": "Outlining and todo lists in Atom (similar to org-mode)",
  "keywords": [
    "organized",
    "org-mode",
    "todo",
    "outline"
  ],
  "repository": "https://github.com/MattFlower/organized",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MattFlower/organized/issues"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "tmp": ">=0.0.28",
    "rx-text-search": ">=1.0.0",
    "atom-package-deps": ">=4.3.1",
    "atom-space-pen-views": "^2.2.0", 
    "space-pen": "^5.1.2"
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        ">0.3.2": "consumeToolBar"
      }
    }
  },
  "extensions": {
    ".coffee": [
      "lib/organized.coffee"
    ],
    ".json": [
      "package.json"
    ]
  },
  "folders": [
    {
      "paths": [
        "",
        "lib",
        "grammars"
      ]
    }
  ],
  "package-deps": [
    "tool-bar"
  ]
}
